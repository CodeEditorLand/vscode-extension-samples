{
	"name": "extension-terminal-sample",
	"displayName": "extension-terminal-sample",
	"version": "0.0.1",
	"private": true,
	"description": "An example usage of the extension terminal API",
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"license": "MIT",
	"publisher": "vscode-samples",
	"main": "./out/extension.js",
	"scripts": {
		"compile": "tsc -p ./",
		"lint": "eslint",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"commands": [
			{
				"command": "extensionTerminalSample.create",
				"title": "Extension Terminal Sample: Create"
			},
			{
				"command": "extensionTerminalSample.clear",
				"title": "Extension Terminal Sample: Clear"
			}
		]
	},
	"activationEvents": [],
	"dependencies": {},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/node": "^20",
		"@types/vscode": "^1.74.0",
		"eslint": "^9.13.0",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.16.0"
	},
	"engines": {
		"vscode": "^1.73.0"
	}
}
