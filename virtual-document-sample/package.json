{
  "name": "virtual-document-sample",
  "displayName": "Cowsay in virtual documents",
  "description": "A sample for virtual document",
  "version": "0.0.1",
  "publisher": "playform",
  "private": "false",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cowsay.say",
        "title": "cowsay"
      },
      {
        "command": "cowsay.backwards",
        "title": "cowsay (â†¹)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "cowsay.backwards",
          "group": "navigation",
          "when": "resourceScheme == cowsay"
        }
      ],
      "commandPalette": [
        {
          "command": "cowsay.backwards",
          "when": "resourceScheme == cowsay"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -watch -p ./",
    "prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
  },
  "dependencies": {
    "cowsay": "1.5.0"
  },
  "devDependencies": {
    "@types/node": "20.9.4",
    "@types/vscode": "1.84.2",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "eslint": "8.54.0",
    "typescript": "5.3.2",
    "typescript-esbuild": "0.3.1"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  }
}
