{
	"name": "virtual-document-sample",
	"displayName": "Cowsay in virtual documents",
	"description": "A sample for virtual document",
	"main": "./out/extension.js",
	"scripts": {
		"compile": "tsc -p ./"
	},
	"contributes": {
		"commands": [
			{
				"command": "cowsay.say",
				"title": "cowsay"
			},
			{
				"command": "cowsay.backwards",
				"title": "cowsay (â†¹)"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "cowsay.backwards",
					"when": "resourceScheme == cowsay"
				}
			],
			"editor/title": [
				{
					"command": "cowsay.backwards",
					"group": "navigation",
					"when": "resourceScheme == cowsay"
				}
			]
		}
	},
	"activationEvents": [],
	"dependencies": {
		"cowsay": "1.6.0"
	},
	"devDependencies": {
		"@types/node": "22"
	}
}
