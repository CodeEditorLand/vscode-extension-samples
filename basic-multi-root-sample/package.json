{
	"name": "basic-multi-root-sample",
	"displayName": "Basic Multi Root Sample",
	"version": "0.0.1",
	"private": true,
	"description": "Samples for VSCode's multi root API",
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"license": "MIT",
	"publisher": "vscode-samples",
	"main": "./out/src/extension",
	"scripts": {
		"compile": "tsc -p ./",
		"lint": "eslint",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"configuration": {
			"properties": {
				"multiRootSample.statusColor": {
					"default": "#FFFFFF",
					"description": "Color to use for the status bar item. Can be set per workspace folder.",
					"scope": "resource",
					"type": [
						"string"
					]
				}
			},
			"title": "Basic Multi Root Sample",
			"type": "object"
		}
	},
	"activationEvents": [
		"*"
	],
	"dependencies": {},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/node": "^20",
		"@types/vscode": "^1.73.0",
		"eslint": "^9.13.0",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.16.0"
	},
	"engines": {
		"vscode": "^1.73.0"
	}
}
