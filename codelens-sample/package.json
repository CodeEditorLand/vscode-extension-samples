{
	"name": "codelens-sample",
	"displayName": "CodeLens Sample",
	"version": "0.0.1",
	"private": true,
	"description": "Samples for VS Code's CodeLens API",
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"license": "MIT",
	"publisher": "ms-vscode",
	"main": "./out/extension",
	"scripts": {
		"compile": "tsc -p ./",
		"lint": "eslint",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"commands": [
			{
				"category": "CodeLens Sample",
				"command": "codelens-sample.enableCodeLens",
				"title": "Enable CodeLens"
			},
			{
				"category": "CodeLens Sample",
				"command": "codelens-sample.disableCodeLens",
				"title": "Disable Codelens"
			}
		],
		"configuration": {
			"properties": {
				"codelens-sample.enableCodeLens": {
					"default": true,
					"type": "boolean"
				}
			}
		}
	},
	"activationEvents": [
		"*"
	],
	"dependencies": {},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/node": "^20",
		"@types/vscode": "^1.73.0",
		"eslint": "^9.13.0",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.16.0"
	},
	"engines": {
		"vscode": "^1.73.0"
	}
}
