{
	"browser": "./out/extension.js",
	"scripts": {
		"compile": "tsc -p ./"
	},
	"contributes": {
		"configuration": {
			"id": "tabsSample",
			"properties": {
				"tabs.maxInactiveTime": {
					"default": 30,
					"description": "The number of seconds before closing inactive tabs",
					"type": "number"
				}
			},
			"title": "Tabs API Sample"
		}
	},
	"activationEvents": [
		"onStartupFinished"
	],
	"devDependencies": {
		"@stylistic/eslint-plugin": "2.11.0",
		"@types/node": "22"
	}
}
