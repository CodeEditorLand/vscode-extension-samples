{
	"name": "l10n-sample",
	"displayName": "l10n-sample",
	"version": "0.1.0",
	"private": true,
	"description": "Sample that shows how to localize an extension",
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"license": "MIT",
	"publisher": "vscode-samples",
	"main": "./out/extension.js",
	"scripts": {
		"compile": "tsc -p ./",
		"lint": "eslint",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"commands": [
			{
				"command": "extension.sayHello",
				"title": "%extension.sayHello.title%"
			},
			{
				"command": "extension.sayBye",
				"title": "%extension.sayBye.title%"
			}
		]
	},
	"activationEvents": [],
	"dependencies": {
		"@codeeditorland/l10n": "^0.0.10"
	},
	"devDependencies": {
		"@codeeditorland/l10n-dev": "^0.0.18",
		"@codeeditorland/test-electron": "^2.3.9",
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "^20",
		"@types/vscode": "^1.73.0",
		"eslint": "^9.13.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.16.0"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"l10n": "./l10n"
}
