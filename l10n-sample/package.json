{
	"name": "l10n-sample",
	"displayName": "l10n-sample",
	"description": "Sample that shows how to localize an extension",
	"main": "./out/extension.js",
	"scripts": {
		"compile": "tsc -p ./"
	},
	"contributes": {
		"commands": [
			{
				"command": "extension.sayHello",
				"title": "%extension.sayHello.title%"
			},
			{
				"command": "extension.sayBye",
				"title": "%extension.sayBye.title%"
			}
		]
	},
	"activationEvents": [],
	"dependencies": {
		"@codeeditorland/l10n": "0.0.1"
	},
	"devDependencies": {
		"@codeeditorland/l10n-dev": "0.0.1",
		"@codeeditorland/test-electron": "0.0.1",
		"@stylistic/eslint-plugin": "2.12.1",
		"@types/glob": "8.1.0",
		"@types/node": "22",
		"glob": "11.0.0"
	},
	"l10n": "./l10n"
}
