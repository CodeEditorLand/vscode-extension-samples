{
	"name": "notebook-renderer-sample",
	"displayName": "notebook-renderer-sample",
	"version": "0.0.1",
	"private": true,
	"description": "Example of a Notebook renderer (visualizer) for GitHub Issues Notebook that doesn't use any frontend frameworks.",
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"license": "MIT",
	"scripts": {
		"lint": "eslint",
		"test-compile": "tsc -p ./",
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch"
	},
	"contributes": {
		"notebookRenderer": [
			{
				"displayName": "GitHub Issues Plain Renderer",
				"entrypoint": "./out/index.js",
				"id": "notebook-renderer-github-issues",
				"mimeTypes": [
					"x-application/github-issues"
				]
			}
		]
	},
	"dependencies": {},
	"devDependencies": {
		"@codeeditorland/notebook-error-overlay": "^1.0.1",
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/land-notebook-renderer": "^1.57.8",
		"@types/webpack-env": "^1.17.0",
		"css-loader": "^4.2.0",
		"eslint": "^9.13.0",
		"style-loader": "^1.2.1",
		"ts-loader": "^9.2.2",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.16.0",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	},
	"engines": {
		"vscode": "^1.58.0"
	}
}
