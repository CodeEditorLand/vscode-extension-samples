{
	"name": "@codeeditorland/memfs",
	"displayName": "MemFS - a file system provider sample",
	"version": "0.0.3",
	"private": true,
	"description": "Showcase for the file system provider API, also useful for testing again document that are not on disk.",
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"license": "MIT",
	"publisher": "vscode-samples",
	"main": "./out/src/extension",
	"scripts": {
		"compile": "tsc -p ./",
		"lint": "eslint",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"commands": [
			{
				"category": "MemFS",
				"command": "memfs.workspaceInit",
				"title": "Setup Workspace"
			},
			{
				"category": "MemFS",
				"command": "memfs.init",
				"title": "Create Files"
			},
			{
				"category": "MemFS",
				"command": "memfs.reset",
				"title": "Delete Files"
			},
			{
				"category": "MemFS",
				"command": "memfs.deleteFile",
				"title": "Delete \"file.txt\""
			},
			{
				"category": "MemFS",
				"command": "memfs.addFile",
				"title": "Add \"file.txt\""
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "memfs.init",
					"when": "workbenchState == workspace"
				},
				{
					"command": "memfs.reset",
					"when": "workbenchState == workspace"
				},
				{
					"command": "memfs.deleteFile",
					"when": "workbenchState == workspace"
				},
				{
					"command": "memfs.addFile",
					"when": "workbenchState == workspace"
				},
				{
					"command": "memfs.workspaceInit",
					"when": "workbenchState != workspace"
				}
			]
		}
	},
	"activationEvents": [
		"onFileSystem:memfs"
	],
	"dependencies": {},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/node": "^20",
		"@types/vscode": "^1.73.0",
		"eslint": "^9.13.0",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.16.0"
	},
	"engines": {
		"vscode": "^1.74.0"
	}
}
