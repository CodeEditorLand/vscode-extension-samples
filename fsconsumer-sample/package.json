{
	"name": "@codeeditorland/memfs",
	"displayName": "FileSystem Consumer Sample",
	"version": "0.0.1",
	"private": true,
	"description": "Showcase for using the workspace.fs.-API which allows reading and writing files and folder",
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"license": "MIT",
	"publisher": "vscode-samples",
	"main": "./out/src/extension",
	"scripts": {
		"compile": "tsc -p ./",
		"lint": "eslint",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"commands": [
			{
				"category": "FS",
				"command": "fs/openJS",
				"title": "Open JavaScript-file for TypeScript-file..."
			},
			{
				"category": "FS",
				"command": "fs/sumSizes",
				"title": "Show folder information for current file"
			},
			{
				"category": "FS",
				"command": "fs/readWriteFile",
				"title": "Read and Write file"
			}
		]
	},
	"activationEvents": [],
	"dependencies": {},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/node": "^20",
		"@types/vscode": "^1.73.0",
		"eslint": "^9.13.0",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.16.0"
	},
	"engines": {
		"vscode": "^1.74.0"
	}
}
